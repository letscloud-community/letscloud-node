{
  "name": "@letscloud/node",
  "version": "0.0.1",
  "description": "Node Api wrapper for letscloud API version 1",
  "main": "dist/index.js",
  "types": "./letscloud-node.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "dev": "ts-node-dev --transpileOnly --ignore-watch node_modules src/index.ts",
    "lint:js": "eslint . --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letscloud-community/letscloud-node.git"
  },
  "keywords": [
    "api",
    "letscloud"
  ],
  "author": "Letscloud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/letscloud-community/letscloud-node/issues"
  },
  "homepage": "https://github.com/letscloud-community/letscloud-node#readme",
  "dependencies": {
    "axios": "~0.21.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "engines": {
    "node": ">= 12"
  }
}
